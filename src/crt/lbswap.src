	assume	adl=1

	section	.text
	public	__lbswap
__lbswap:
	push	hl			; ...[hlu]hl <- (sp)
	inc	sp			; ...[hlu]h <- (sp)
	ld	h, e			; uhl = [hlu]el
	push	hl			; ...[hlu]h[hlu]el <- (sp)
	inc	sp			; ...[hlu]h[hlu]e <- (sp)
	ld	e, l
	pop	hl			; euhl = lh[hlu]e
					; ...[hlu] <- (sp)
	inc	sp			; ... <- (sp)
	ret
